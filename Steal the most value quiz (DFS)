#include<iostream>
using namespace std;
#define INF 1e12;
int n,k,ans= INF;
int v[15], w[15];
void dfs (int i, int sumv, int sumw)
{
    if(sumv>=k){
        ans= min(ans, sumw);
        return;
    }
    if(i==n) return;
    dfs(i+1, sumv, sumw);
    dfs(i+1, sumv+v[i], sumw+w[i]);

        
    
}
int main()
{
    cin>>n>>k;
    for (int i=0; i<n; i++)
    {
        cin>>v[i]>>w[i];
    }
    dfs(0,0,0);
    cout<<ans<<"/n";
}
